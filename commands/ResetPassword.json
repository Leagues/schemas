{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ResetPassword",
  "description": "Resets the password of an account",
  "type": "object",
  "properties": {
    "token": {
      "format": "uuid"
    },
    "newPassword": {
      "type": "string",
      "minLength": 8
    }
  },
  "required": ["token","newPassword"]
}